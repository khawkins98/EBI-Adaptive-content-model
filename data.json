---
layout: null
---
{"nodes":[
  {% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "Test": "{{post.parent-pages}}",
      "parent-pages":  "{% for parent-page in post.parent-pages %}{{ parent-page }}{% unless forloop.last %}, {% endunless %}{% endfor %}",
      "users": "{% for user in post.users %}{{ user }}{% unless forloop.last %}, {% endunless %}{% endfor %}",
      "goals": "{% for goal in post.goals %}{{ goal }}{% unless forloop.last %}, {% endunless %}{% endfor %}",
      "url": "{{ post.url }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]}
